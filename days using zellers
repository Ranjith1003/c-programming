#include <stdio.h>

// Function to find the day of the week
int findDay(int day, int month, int year) {
    int m = month;
    int y = year;

    // Adjust month and year for the algorithm
    if (m == 1 || m == 2) {
        m += 12;
        y -= 1;
    }

    int k = day;
    int yearOfCentury = y % 100;
    int century = y / 100;
    int f = k + ((13 * (m + 1)) / 5) + yearOfCentury + (yearOfCentury / 4) + (century / 4) - (2 * century);
    int dayOfWeek = (f + 5) % 7; 

    return dayOfWeek;
}

int main() {
    int day, month, year;

    
    printf("Enter the date (DD MM YYYY): ");
    scanf("%d %d %d", &day, &month, &year);

    
    int dayOfWeek = findDay(day, month, year);

    // Output the day of the week
    switch (dayOfWeek) {
        case 0:
            printf("Sunday\n");
            break;
        case 1:
            printf("Monday\n");
            break;
        case 2:
            printf("Tuesday\n");
            break;
        case 3:
            printf("Wednesday\n");
            break;
        case 4:
            printf("Thursday\n");
            break;
        case 5:
            printf("Friday\n");
            break;
        case 6:
            printf("Saturday\n");
            break;
    }

    return 0;
}
